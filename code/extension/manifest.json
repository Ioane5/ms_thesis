{
  "manifest_version": 2,
  "name": "ChatApp",
  "description": "This is Chat App extension, that will be using P2P implementation",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Start Chat!"
  },
  "background": {
    "scripts": [
      "dataController.js",
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "background",
    "storage",
    "alarms",
    "unlimitedStorage"
  ]
}